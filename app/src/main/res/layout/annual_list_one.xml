<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">

    <TextView
        android:gravity="center|left"
        android:layout_marginLeft="15dp"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="企业名称"
        style="@style/tv_top"/>
    <TextView
        android:id="@+id/company"
        android:layout_marginLeft="15dp"
        style="@style/tv_down"
        android:layout_marginTop="-5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""/>
    <ImageView
        android:layout_marginLeft="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/line"/>
    <TextView
        android:gravity="center|left"
        android:layout_marginLeft="15dp"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="注册号"
        style="@style/tv_top"/>
    <TextView
        android:id="@+id/regId"
        android:layout_marginLeft="15dp"
        style="@style/tv_down"
        android:layout_marginTop="-5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""/>
    <ImageView
        android:layout_marginLeft="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/line"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:gravity="center|left"
                android:layout_marginLeft="15dp"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="企业联系电话"
                style="@style/tv_top"/>
            <TextView
                android:id="@+id/phone"
                android:layout_marginLeft="15dp"
                style="@style/tv_down"
                android:layout_marginTop="-5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""/>
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:gravity="center|left"
                android:layout_marginLeft="15dp"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="邮政编码"
                style="@style/tv_top"/>
            <TextView
                android:id="@+id/zipCode"
                android:layout_marginLeft="15dp"
                style="@style/tv_down"
                android:layout_marginTop="-5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""/>
        </LinearLayout>

    </LinearLayout>
    <ImageView
        android:layout_marginLeft="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/line"/>
    <TextView
        android:gravity="center|left"
        android:layout_marginLeft="15dp"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="企业通信地址"
        style="@style/tv_top"/>
    <TextView
        android:id="@+id/address"
        android:layout_marginLeft="15dp"
        style="@style/tv_down"
        android:layout_marginTop="-5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""/>
    <ImageView
        android:layout_marginLeft="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/line"/>
    <TextView
        android:gravity="center|left"
        android:layout_marginLeft="15dp"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="电子邮箱"
        style="@style/tv_top"/>
    <TextView
        android:id="@+id/email"
        android:layout_marginLeft="15dp"
        style="@style/tv_down"
        android:layout_marginTop="-5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""/>
    <ImageView
        android:layout_marginLeft="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/line"/>
    <TextView
        android:gravity="center|left"
        android:layout_marginLeft="15dp"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="有限责任公司本年度是否发生股权转让"
        style="@style/tv_top"/>
    <TextView
        android:id="@+id/gqzr"
        android:layout_marginLeft="15dp"
        style="@style/tv_down"
        android:layout_marginTop="-5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""/>
    <ImageView
        android:layout_marginLeft="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/line"/>
    <TextView
        android:gravity="center|left"
        android:layout_marginLeft="15dp"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="企业经营状况"
        style="@style/tv_top"/>
    <TextView
        android:id="@+id/state"
        android:layout_marginLeft="15dp"
        style="@style/tv_down"
        android:layout_marginTop="-5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""/>
    <ImageView
        android:layout_marginLeft="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/line"/>
    <TextView
        android:gravity="center|left"
        android:layout_marginLeft="15dp"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="是否有网站或网店"
        style="@style/tv_top"/>
    <TextView
        android:id="@+id/online"
        android:layout_marginLeft="15dp"
        style="@style/tv_down"
        android:layout_marginTop="-5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""/>
    <ImageView
        android:layout_marginLeft="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/line"/>
    <TextView
        android:gravity="center|left"
        android:layout_marginLeft="15dp"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="企业是否有投资信息或购买其它公司股权"
        style="@style/tv_top"/>
    <TextView
        android:id="@+id/tzxx"
        android:layout_marginLeft="15dp"
        style="@style/tv_down"
        android:layout_marginTop="-5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""/>
    <ImageView
        android:layout_marginLeft="15dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/line"/>
    <TextView
        android:gravity="center|left"
        android:layout_marginLeft="15dp"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="从业人数"
        style="@style/tv_top"/>
    <TextView
        android:id="@+id/number"
        android:layout_marginLeft="15dp"
        style="@style/tv_down"
        android:layout_marginTop="-5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""/>
</LinearLayout>
